mplementation 'com.github.Nikoloutsos:KLoadingSpin:1.0.0'\n\n    //todo dot indicator\n    implementation \"com.tbuonomo:dotsindicator:$dotIndicatorVersion\"\n\n    //todo OTP\n    implementation \"com.chaos.view:pinview:$otpView\"\n\n    //Todo Calendar\n    implementation 'com.github.tizisdeepan:eventscalendar:1.6.1'\n\n    //todo bottom navigation\n    implementation 'com.github.irfaan008:IRBottomNavigationView:1.0.1'\n\n\n    //Todo Material Dialogs\n    implementation 'com.afollestad.material-dialogs:core:3.3.0'\n    implementation 'com.afollestad.material-dialogs:lifecycle:3.3.0'\n    implementation 'com.afollestad.material-dialogs:datetime:3.3.0'\n    implementation 'com.afollestad.material-dialogs:bottomsheets:3.3.0'\n\n\n    //TODO Import the BoM for the Firebase platform\n\n    implementation platform('com.google.firebase:firebase-bom:28.4.0')\n    implementation 'com.google.android.gms:play-services-auth:20.0.1'\n    implementation 'com.google.firebase:firebase-core:20.1.0'\n    implementation 'com.google.firebase:firebase-storage:20.0.0'\n    implementation 'com.google.firebase:firebase-firestore:24.0.1'\n    implementation 'com.google.firebase:firebase-auth:21.0.1'\n    implementation 'com.google.firebase:firebase-auth-ktx'\n    implementation 'com.google.firebase:firebase-auth-ktx:21.0.1'\n    implementation 'androidx.browser:browser:1.4.0'\n    implementation 'com.google.firebase:firebase-analytics-ktx'\n    //todo firebase messaging\n    implementation 'com.google.firebase:firebase-messaging:23.0.0'\n\n\n\n    //todo navigation drawer\n    implementation 'nl.psdcompany:duo-navigation-drawer:3.0.0'\n    //todo filepicker\n    implementation 'com.github.dhaval2404:imagepicker:2.1'\n\n    implementation 'de.hdodenhof:circleimageview:3.1.0'\n    implementation \"com.github.marlonlom:timeago:4.0.3\"\n    implementation 'com.github.bumptech.glide:glide:4.13.0'\n    annotationProcessor 'com.github.bumptech.glide:compiler:4.13.0'\n\n\n\n\n}
